<Application x:Class="Corvinus.WPF.UI.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Corvinus.WPF.UI"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="\Resources\Localization\en-US\StringResources.xaml" />
                <ResourceDictionary Source="\Resources\Themes\Default.xaml" />
                <ResourceDictionary Source="\Resources\Base.xaml" />
                <ResourceDictionary Source="\Resources\ControlStyles.xaml" />
                <ResourceDictionary Source="\Resources\ImageGeometry.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
